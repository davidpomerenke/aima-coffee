{
  "name": "aima",
  "version": "0.6.0",
  "author": "David Pomerenke",
  "description": "Implementation of Russell & Norvig, \"Artificial Intelligence - A Modern Approach\". ",
  "repository": {
    "url": "https://github.com/davidpomerenke/aima.js"
  },
  "license": "ISC",
  "keywords": [
    "Artificial Intelligence",
    "Stuart Russell",
    "Peter Norvig",
    "Intelligent Agents",
    "Problem-Solving",
    "Search",
    "Optimization",
    "Games"
  ],
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "npm run prepare && node --require source-map-support/register index.js",
    "prepare": "coffee --compile --transpile --inline-map --output index.js readme.litcoffee",
    "build-docs": "docco readme.litcoffee --layout linear",
    "coverage": "coffee --compile --output index.mjs readme.litcoffee && c8 node index.mjs --reporter=text-lcov > coverage.lcov"
  },
  "engines": {
    "node": ">= 13.0"
  },
  "dependencies": {
    "deep-equal": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "coffeescript": "^2.5.1",
    "docco": "^0.8.0",
    "random-seed": "^0.3.0",
    "source-map-support": "^0.5.16"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
