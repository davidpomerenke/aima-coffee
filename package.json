{
  "name": "aima",
  "version": "0.6.6",
  "author": "David Pomerenke",
  "description": "Implementation of Russell & Norvig, \"Artificial Intelligence - A Modern Approach\". ",
  "repository": {
    "url": "https://github.com/davidpomerenke/aima-coffee"
  },
  "homepage": "https://davidpomerenke.github.io/aima-coffee",
  "license": "ISC",
  "keywords": [
    "Artificial Intelligence",
    "Stuart Russell",
    "Peter Norvig",
    "Intelligent Agents",
    "Problem-Solving",
    "Search",
    "Optimization",
    "Games"
  ],
  "type": "module",
  "module": "index.mjs",
  "main": "index.mjs",
  "files": [
    "index.mjs",
    "readme.md"
  ],
  "scripts": {
    "test": "coffee --compile --print readme.litcoffee | node --input-type=module --require source-map-support/register",
    "prepare": "cat readme.litcoffee | sed -e 's/^.*# testing only$//g' | coffee --stdio --literate --compile --transpile > index.mjs",
    "build-docs": "docco readme.litcoffee --layout linear && mv docs/readme.html docs/index.html",
    "coverage": "npm run prepare && c8 node index.mjs --reporter=text-lcov > coverage.lcov"
  },
  "engines": {
    "node": ">= 13.0"
  },
  "dependencies": {
    "deep-equal": "^1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "babel-plugin-unassert": "^3.0.1",
    "c8": "^7.0.1",
    "coffeescript": "^2.5.1",
    "docco": "^0.8.0",
    "random-seed": "^0.3.0",
    "source-map-support": "^0.5.16"
  },
  "babel": {
    "plugins": [
      "unassert"
    ]
  }
}
